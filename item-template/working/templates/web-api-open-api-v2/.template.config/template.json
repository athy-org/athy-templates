{
  "$schema": "http://json.schemastore.org/template",
  "author": "Me",
  "classifications": [
    "Common",
    "WebAPI",
    "C#10"
  ],
  "identity": "Athy.Template.OpenAPI.API",
  "name": "Athy OpenAPI Web API Template",
  "shortName": "athy-web-api-open-api",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "symbols": {
    "ControllerName": {
      "type": "parameter",
      "fileRename": "Athy__SampleController",
      "replaces": "Athy__SampleController"
    },
    "ControllerRoute": {
      "type": "parameter",
      "replaces": "Athy__ControllerRoute"
    },
    "RequestContentType": {
      "type": "parameter",
      "replaces": "Athy__ContentType"
    },    
    "Route": {
      "type": "parameter",
      "replaces": "Athy__EndpointRoute"
    },
    "ReturnTypeName": {
      "type": "parameter",
      "replaces": "Athy__ReturnTypeName",
      "fileRename": "Athy__ReturnTypeName"
    },
    "ReturnTypeBody": {
      "type": "parameter",
      "replaces": "Athy__ReturnTypeBody"
    },
    "MethodName": {
      "type": "parameter",
      "replaces": "Athy__EndpointMethodName"
    }, 
    "Signature":{
      "type": "parameter",
      "replaces": "Athy__EndpointSignature",
      "description": "Use to represent the endpoints entire signature. Code will be filled in by the generator according to the spec and conventions.",
      "isRequired": false,
      "datatype": "text",
      "defaultValue": ""      
    },
    "Description":{
      "type": "parameter",
      "replaces": "Athy__EndpointDescription",
      "isRequired": false,
      "datatype": "text",
      "defaultValue": ""
    },
    "HttpMethod": {
      "type": "parameter",
      "replaces": "Athy__HttpMethod"
    },
    "SwaggerResponse": {
      "type": "parameter",
      "replaces": "[Athy__SwaggerResponse]"
    },
    "MethodReturnExpression": {
      "type": "parameter",
      "replaces": "Athy__ReturnExpression"
    },
    "athy__EndpointSignatureParam":{
      "type": "parameter",
      "replaces": "athy__EndpointSignatureParam",
      "isRequired": false,      
      "datatype": "text",
      "defaultValue": ""
    }
  }
}